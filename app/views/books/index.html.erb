<h1>Who Wants To Read?</h1>
    <h2><%= @books.each do |book| %></h2>
      <div>
        <%= book.title%>
        <%= book.author %>
        <%= book.genre %>
        <%= book.classification %>
        <%= book.book_type %>
        <%= book.year %>
        <%= link_to 'Show', "#" %>
        <%= link_to 'Edit', "#" %>
        <%= link_to 'Destroy', method: :delete %>
      </div>
    <% end %>

<%= form_tag("/search", method: "get") do %>
  <%= label_tag(:q, "Search for:") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag("Search") %>
<% end %>