<h1>New Author</h1>

    <%= form_for @author, "#" do%>
        <%= f.label :first_name %>
        <%= f.text_field : first_name %>
        <br/>

        <%= f.label :last_name %>
        <%= f.text_field :last_name %>
        <br/>

        <%= f.label :age %>
        <%= f.text_field :age %>
        <br/>

        <%= f.submit "Create",'#'%>

    <% end %>        